// find min and max value from an array
#include<stdio.h>
int main() {
	int arr[]={14, 2, 21, 33, 11, 1};
	int n = sizeof(arr) / sizeof(arr[0]);
	
	int i, max=arr[0], min=arr[0];
	printf("\nFind max");
	for(i=0; i<n-1; i++) {
		
		if(max > arr[i+1]) {
			max = max;
		} else {
			max = arr[i+1];
		}
	} printf("\n%d",max);
	
	printf("\nFind min");

	for(i=0; i<n-1; i++) {
		if(min < arr[i+1]) {
			min = min;
		} else {
			min = arr[i+1];
		}
	} printf("\n%d",min);
	
}
