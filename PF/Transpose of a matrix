// Write a program to transpose a matrix.
#include<stdio.h>
int main() {
    int row = 2, col = 3;
    int matrix[2][3] = {
        {1, 2, 3},
        {4, 5, 6}
    };
    int i, j;
    printf("\nTranspose of the matrix:\n");
    for(i = 0; i < col; i++) {   
        for(j = 0; j < row; j++) { 
            printf("%d ", matrix[j][i]); 
        }
        printf("\n");
    }
    return 0;
}
